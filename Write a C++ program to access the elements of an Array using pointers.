#include <iostream>
using namespace std;
int main()
{
    int *p, arr[5], i;

    cout << "Enter the elements of the array:" << endl;
    for (i = 0; i < 5; i++)
    {
        cin >> arr[i];
    }
    p = arr;

    cout << endl;
    cout << "Entered elements are:" << endl;
    i = 0;
    for (i = 0; i < 5; i++)
    {
        cout << *p << endl;
        p++;
    }
    return 0;
}
